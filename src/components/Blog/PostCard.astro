---
const { frontmatter, url } = Astro.props;
const { title, date, description, author, minutesRead } = frontmatter;

const allPosts = await Astro.glob('../../content/blog/*.md');
---

<div>
    <a href={url}>
        <p>{title}</p>
        <p>{date}</p>
        <p>{description}</p>
        <p>{author}</p>
        <p>{minutesRead}</p>
    </a>
</div>
